{
  "name": "Product Development Container",
  "dockerFile": "Dockerfile",
  "context": "..",
  "appPort": [8000],
  "extensions": [
    "ms-python.python",
    "ms-toolsai.jupyter"
  ],
  "settings": {
    "python.pythonPath": "/usr/local/bin/python"
  },
  "postCreateCommand": "pip install -r requirements.txt"
}